@model IEnumerable<ParcelDTO>
<div class="render-body" style="padding-left: 100px; padding-right: 100px;">
    <div class="container-fluid">
        <div class="row">
            <div class="col flex-sm-wrap-reverse align-content-center" style="margin-right: 80px;">
                <img src="@Constants.DOMAIN_NAME/images/customer-parcels.png" alt="DeliverIT login image" style="width:100%; height: auto; min-width: 400px;" />
            </div>
            <div class="col" style="width:auto; height:400px; overflow:auto; padding: 15px;">
                <table class="table-custom">
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Category Name</th>
                        <th scope="col">Shipment Id</th>
                        <th scope="col">Warehouse Id</th>
                        <th scope="col">Weight</th>
                        <th scope="col">Deliver to Address</th>
                    </tr>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <th scope="col">@item.Id</th>
                            <td scope="col">@item.CategoryName</td>
                            <td scope="col">@item.ShipmentId</td>
                            <td scope="col">@item.WareHouseId</td>
                            <td scope="col">@item.Weight</td>
                            <td scope="col">@item.DeliverToAddress</td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>
</div>